
@{
    ViewBag.Title = "WelcomePage";
}
@if (Session["LoginName"] != null)
{
    <h2>Welcome @Session["LoginName"].ToString()</h2>
    <h3>Full Name @Session["FullName"].ToString()</h3>
   
}

<div class="form-group">
    <div class="col-md-offset-2 col-md-10">
        <input type="submit" value="Logout" class="btn btn-default" />
    </div>
</div>
<div>
    @Html.ActionLink("Update Profile", "Edit", new { LoginName = Model.LoginName })
</div>


